{
    "name": "ck-developer/aist-alice-fixtures",
    "description": "Alice Fixtures for ZF2.",
    "keywords": [
        "alice",
        "fixtures",
        "faker",
        "zf2",
        "developer tools"
    ],
    "authors": [
        {
            "name": "MaSi",
            "email": "webdeveloper@mateuszsitek.com",
            "homepage": "http://mateuszsitek.com",
            "role": "Author"
        }
    ],
    "require": {
        "php": ">=5.5",
        "zendframework/zend-modulemanager": "2.*",
        "doctrine/doctrine-orm-module": "0.9.*",
        "fzaninotto/faker": "1.6.*",
        "nelmio/alice": "2.1.*",
        "bacon/bacon-string-utils": "1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "5.3.*"
    },
    "autoload": {
        "psr-0": {
            "AistAliceFixtures": "src/"
        },
        "classmap": [
            "./Module.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "AistAliceFixturesTest\\": "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }

}
